    "description": "cjango wechat sdk for thinkphp",
    "keywords": ["cjango", "wechat", "sdk"],
    "type": "think-extend",
    "homepage": "https://github.com/cjango/think-wechat",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason.Chen",
            "email": "chenjxlg@163.com",
            "homepage": "https://www.cjango.com/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
